<launch>
    <node name="automow_ekf" pkg="automow_ekf" type="ekf_node.py"
        output="screen" >
        <param name="odom_used" value="true"/>
        <param name="imu_used" value="true"/>
        <param name="gps_used" value="true"/>

        <param name="decimate_ahrs_by_factor" value="10"/>
        <param name="publish_rate" value="25"/>

        <param name="output_tf" value="true"/>
        <param name="output_frame" value="/odom"/>

        <param name="adaptive_encoders" value="true"/>
        <param name="publish_states" value="true"/>
    </node>
</launch>
